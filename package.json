{
  "name": "mop-ui",
  "version": "1.0.0",
  "description": "UI5 Application: MOP",
  "author": "cristian.triscu",
  "license": "Apache-2.0",
  "scripts": {
    "build": "ui5 build --clean-dest",
    "build:opt": "ui5 build self-contained --clean-dest --all",
    "build:uimodule": "ui5 build --config=ui5.yaml --clean-dest --dest uimodule/dist --include-task=generateManifestBundle",
    "start": "ui5 serve --port 8080 -o index.html",
    "start:dist": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
    "ts-typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "build:mta": "mbt build",
    "deploy:cf": "cross-var cf deploy mta_archives/DXCWcore_$npm_package_version.mtar",
    "deploy": "run-s build:mta deploy:cf"
  },
  "devDependencies": {
    "@sapui5/ts-types-esm": "1.108.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@ui5/cli": "^2.14.14",
    "eslint": "^8.27.0",
    "typescript": "^4.8.4",
    "ui5-middleware-livereload": "^0.7.0",
    "ui5-tooling-transpile": "^0.2.0",
    "ui5-task-zipper": "^0.4.7"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload",
      "ui5-tooling-transpile",
      "ui5-task-zipper"
    ]
  }
}